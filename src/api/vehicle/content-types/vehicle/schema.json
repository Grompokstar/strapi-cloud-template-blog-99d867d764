        "rear",
        "all"
      ]
    },
    "modification": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::modification.modification",
      "inversedBy": "vehicles"
    },
    "options": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::option.option",
      "inversedBy": "vehicles"
    },
    "name": {
      "type": "string"
    },
    "photos": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    }
